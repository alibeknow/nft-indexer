{
  "name": "nft-indexer-evm",
  "version": "1.0.0",
  "description": "",
  "main": "src/app-indexer/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "api": "npx nest start api --watch | npx pino-pretty --colorize --translateTime",
    "api:run": "ts-node -r tsconfig-paths/register src/api/main.ts",
    "api:docker": "node -r ./paths-overwrite.js dist/api/main.js",
    "app-events-reader": "ts-node -r tsconfig-paths/register src/app-events-reader/main.ts",
    "app-events-reader:docker": "node -r ./paths-overwrite.js dist/app-events-reader/main.js",
    "api-internal": "npx nest start api-internal --watch | npx pino-pretty --colorize --translateTime",
    "api-internal:run": "ts-node -r tsconfig-paths/register src/api-internal/main.ts",
    "api-internal:docker": "node -r ./paths-overwrite.js dist/api-internal/main.js",
    "app-indexer": "ts-node -r tsconfig-paths/register src/app-indexer/main.ts",
    "app-indexer:docker": "node -r ./paths-overwrite.js dist/app-indexer/main.js",
    "app-lazy-mint-indexer": "ts-node -r tsconfig-paths/register src/app-lazy-mint-indexer/main.ts nft-lazy-mint-indexer --block-number-from 15197632 --block-number-to 15198632 --limit 50",
    "app-lazy-mint-indexer:docker": "node -r ./paths-overwrite.js dist/app-lazy-mint-indexer/main.js nft-lazy-mint-indexer --block-number-from $LAZY_MINT_BLOCK_NUMBER_FROM --block-number-to $LAZY_MINT_BLOCK_NUMBER_TO --limit $LAZY_MINT_LIMIT",
    "app-contract-reader": "ts-node -r tsconfig-paths/register src/app-contract-reader/main.ts",
    "app-contract-reader:docker": "node -r ./paths-overwrite.js dist/app-contract-reader/main.js",
    "app-metadata-reader": "ts-node -r tsconfig-paths/register src/app-metadata-reader/main.ts nft-metadata-reader --check-interval 10 --workers 10 --batch-limit 100 --run-loop true",
    "app-metadata-reader:queue": "ts-node -r tsconfig-paths/register src/app-metadata-reader/main.ts nft-metadata-reader --check-interval 10 --workers 10 --batch-limit 100",
    "app-metadata-reader:docker": "node -r ./paths-overwrite.js ./dist/app-metadata-reader/main.js nft-metadata-reader --check-interval $METADATA_READER_CHECK_INTERVAL --workers $METADATA_READER_WORKERS --batch-limit $METADATA_READER_BATCH_LIMIT --run-loop true",
    "app-metadata-reader:docker:queue": "node -r ./paths-overwrite.js ./dist/app-metadata-reader/main.js nft-metadata-reader --check-interval $METADATA_READER_CHECK_INTERVAL --workers $METADATA_READER_WORKERS --batch-limit $METADATA_READER_BATCH_LIMIT",
    "app-pre-contracts": "ts-node -r tsconfig-paths/register src/app-pre-contracts/main.ts",
    "app-pre-contracts:docker": "node -r ./paths-overwrite.js dist/app-pre-contracts/main.js",
    "app-cryptopunks": "ts-node -r tsconfig-paths/register src/app-cryptopunks/cryptopunks.ts",
    "app-opensearch-indexer": "ts-node -r tsconfig-paths/register src/app-opensearch-indexer/main.ts nft-opensearch-indexer",
    "app-opensearch-indexer:docker": "node -r ./paths-overwrite.js dist/app-opensearch-indexer/main.js nft-opensearch-indexer",
    "app-stax-statistics": "ts-node -r tsconfig-paths/register src/app-stax-statistics/main.ts nft-stax-statistics",
    "app-stax-statistics:docker": "node -r ./paths-overwrite.js dist/app-stax-statistics/main.js nft-stax-statistics",
    "app-refresh": "ts-node -r tsconfig-paths/register src/app-refresh/main.ts",
    "build": "npx tsc -p tsconfig.json",
    "test": "npx jest -c jest.config.js",
    "test:unit": "npm run test unit",
    "test:docker": "docker-compose -f docker-compose.test.yaml up --remove-orphans --abort-on-container-exit --exit-code-from test test",
    "test:docker:go": "docker-compose -f docker-compose.test.yaml up --abort-on-container-exit --exit-code-from gotest gotest",
    "lint": "npx eslint -c .eslintrc.json src --ext .ts,.js src test jest.config.js",
    "lint:fix": "npx eslint -c .eslintrc.json src --ext .ts,.js src test jest.config.js --fix",
    "start:docker": "docker-compose up -d",
    "stop:docker": "docker-compose down -v",
    "start:mongo": "docker-compose up -d mongodb",
    "start:rabbit": "docker-compose up -d rabbitmq",
    "start:opensearch": "docker-compose up -d opensearch",
    "sender": "ts-node -r tsconfig-paths/register src/queue-sender/index.ts queue-sender --queue-name QUEUE_NAME --event-type EVENT_TYPE --payload QUEUE_PAYLOAD",
    "testsuite:rarible": "ts-node -r tsconfig-paths/register ./src/testsuite/index.ts rarible-comparison --rarible-url https://api.rarible.org --indexer-url http://localhost:8090 --workers 20",
    "testsuite:rarible:docker": "node -r ./paths-overwrite.js ./dist/testsuite/index.js rarible-comparison --rarible-url https://api.rarible.org --indexer-url http://localhost:$API_PORT --workers 20",
    "tokens-reader:metadata": "ts-node -r tsconfig-paths/register ./src/tokens-reader/index.ts fill-metadata-queue --batch-limit 100 --missing-metadata true",
    "tokens-reader:metadata:docker": "node -r ./paths-overwrite.js ./dist/tokens-reader/index.js fill-metadata-queue --id $TOKENS_READER_ID --created-at $TOKENS_READER_CREATED_AT --batch-limit $TOKENS_READER_BATCH_LIMIT --missing-metadata $TOKENS_READER_MISSING_METADATA",
    "tokens-reader:contract": "ts-node -r tsconfig-paths/register ./src/tokens-reader/index.ts fill-contract-queue --batch-limit 100",
    "tokens-reader:contract:docker": "node -r ./paths-overwrite.js ./dist/tokens-reader/index.js fill-contract-queue --id $TOKENS_READER_ID --created-at $TOKENS_READER_CREATED_AT --batch-limit $TOKENS_READER_BATCH_LIMIT",
    "tokens-reader:count-uris": "ts-node -r tsconfig-paths/register ./src/tokens-reader/index.ts count-token-uris --batch-limit 1000 --start 13915630 --end 14116233 --missing-metadata true",
    "tokens-reader:count-uris:docker": "node -r ./paths-overwrite.js ./dist/tokens-reader/index.js count-token-uris --id $TOKENS_READER_ID --batch-limit $TOKENS_READER_BATCH_LIMIT --start $TOKENS_READER_BLOCK_START --end $TOKENS_READER_BLOCK_END --missing-metadata $TOKENS_READER_MISSING_METADATA",
    "docker:build:metadata-reader": "docker build -f metadata-reader.Dockerfile -t metadata-reader .",
    "docker:compose:build:metadata-reader": "docker compose build nft-metadata-reader --no-cache",
    "start:metadata-reader-go": "docker compose up -d nft-metadata-reader-go"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LedgerHQ/nft-indexer-evm.git"
  },
  "author": "",
  "license": "ISC",
